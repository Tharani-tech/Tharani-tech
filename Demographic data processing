import pandas as pd
import seaborn as sns
import matplotlib.pyplot as plt
df = pd.read_csv("Medicalexamination.csv", 'w')
preweight = df.filter(like ='weight' and 'variable')
weight = preweight / 100
height = df.filter(like ='height' and 'variable')
BMI = weight / (height**2)
if BMI >25:
    print(1)
else:
    print(0)
df = {'Overweight' : 'BMI'}
